{
    "name": "fasnacht-rocks",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "date-fns": "^4.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-modal": "^3.16.3",
        "react-places-autocomplete": "^7.2.1",
        "react-scripts": "5.0.1",
        "styled-components": "^6.0.7"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "release": "git push origin master:release",
        "generate-ics": "node scripts/generate-ics.js",
        "deploy": "npm run generate-ics && git add public/events.ics && git commit -m 'Update events.ics' && git push && npm run release"
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "no-unused-vars": "warn"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "ics": "^3.8.1",
        "netlify-cli": "^18.0.0",
        "sass": "^1.45.0"
    }
}
